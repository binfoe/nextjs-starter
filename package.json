{
  "name": "nextjs-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "db:migrate:dev": "dotenv -e .env.development -- pnpm exec prisma migrate dev",
    "db:migrate:deploy": "dotenv -e .env.production -- pnpm exec prisma migrate deploy",
    "db:generate": "prisma generate",
    "stylelint": "stylelint './src/**/*.scss'",
    "eslint": "next lint --fix",
    "lint": "pnpm eslint && pnpm stylelint",
    "prepare": "husky install && pnpm db:generate",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@prisma/client": "^5.6.0",
    "cva": "npm:class-variance-authority@^0.7.0",
    "lodash-es": "^4.17.21",
    "next": "^14.0.3",
    "preline-react": "^0.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-icons": "^4.12.0",
    "superjson": "^2.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@types/lodash-es": "^4.17.11",
    "@types/node": "^20.9.3",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.16",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.54.0",
    "eslint-config-next": "^14.0.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "prisma": "^5.6.0",
    "tailwindcss": "^3.3.5",
    "type-fest": "^4.8.2",
    "typescript": "^5.3.2",
    "typescript-plugin-css-modules": "^5.0.2"
  }
}
