{
  "name": "nextjs-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "db:migrate:dev": "dotenv -e .env.development -- pnpm exec prisma migrate dev",
    "db:migrate:deploy": "dotenv -e .env.production -- pnpm exec prisma migrate deploy",
    "db:generate": "prisma generate",
    "stylelint": "stylelint './src/**/*.scss'",
    "eslint": "next lint --fix",
    "lint": "pnpm eslint && pnpm stylelint",
    "prepare": "husky install && pnpm db:generate",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.5.0",
    "@hookform/resolvers": "^3.3.4",
    "@prisma/client": "^5.11.0",
    "cva": "npm:class-variance-authority@^0.7.0",
    "google-auth-library": "^9.7.0",
    "lodash-es": "^4.17.21",
    "next": "^14.1.4",
    "next-auth": "5.0.0-beta.15",
    "preline-react": "^0.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.1",
    "react-icons": "^5.0.1",
    "superjson": "^2.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "prisma": "^5.11.0",
    "tailwindcss": "^3.4.1",
    "type-fest": "^4.13.1",
    "typescript": "^5.4.3"
  }
}
