{
  "name": "nextjs-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "db:migrate:dev": "dotenv -e .env.development -- pnpm exec prisma migrate dev",
    "db:migrate:deploy": "dotenv -e .env.production -- pnpm exec prisma migrate deploy",
    "db:generate": "prisma generate",
    "stylelint": "stylelint './src/**/*.scss'",
    "eslint": "next lint --fix",
    "lint": "pnpm eslint && pnpm stylelint",
    "prepare": "husky install && pnpm db:generate",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@prisma/client": "^5.5.1",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.42.0",
    "@trpc/react-query": "^10.42.0",
    "@trpc/server": "^10.42.0",
    "cva": "npm:class-variance-authority@^0.7.0",
    "lodash-es": "^4.17.21",
    "next": "^13.5.6",
    "preline-react": "^0.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-icons": "^4.11.0",
    "superjson": "^2.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.6",
    "@tanstack/react-query-devtools": "^4.36.1",
    "@types/lodash-es": "^4.17.10",
    "@types/node": "^20.8.8",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.52.0",
    "eslint-config-next": "^13.5.6",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "preline": "^1.9.0",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "prisma": "^5.5.1",
    "tailwindcss": "^3.3.4",
    "type-fest": "^4.6.0",
    "typescript": "^5.2.2",
    "typescript-plugin-css-modules": "^5.0.2"
  }
